<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="182.0" prefWidth="352.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sdy.fileSystem.controller.NewFile">
   <children>
      <Text layoutX="35.0" layoutY="61.0" strokeType="OUTSIDE" strokeWidth="0.0" text="新建文件名:">
         <font>
            <Font size="19.0" />
         </font>
      </Text>
      <TextField fx:id="newFileName" layoutX="145.0" layoutY="42.0">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Button defaultButton="true" fx:id="saveNewFileButton" layoutX="73.0" layoutY="109.0" mnemonicParsing="false" onAction="#saveNewFile" text="保存">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="returnButton" layoutX="224.0" layoutY="109.0" mnemonicParsing="false" onAction="#returnNewFile" text="返回">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
